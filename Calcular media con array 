#include <stdio.h>

#define DIM 5

int main() 
{
    int datos[DIM][DIM] = {{1, 2, 5, 9, 5},{5, 4 , 3, 2, 1},{7, 4 ,1, 9, 3}};
    
    
    int media;
    int media1;
    int media2;
    
    media = (datos[0][0] + datos[0][1] + datos[0][2] + datos[0][3] + datos[0][4]) / 5;
    printf("Media de las últimas 5 partidas con Kassadin: %d\n", media);
    
    media1 = (datos[1][0] + datos[1][1] + datos[1][2] + datos[1][3] + datos[1][4]) / 5;
    printf("Media de las últimas 5 partidas con Ahri: %d\n", media1);
    
    media2 = (datos[2][0] + datos[2][1] + datos[2][2] + datos[2][3] + datos[2][4]) / 5;
    printf("Media de las últimas 5 partidas con Jax: %d\n", media2);
    
    
    return 0;
}